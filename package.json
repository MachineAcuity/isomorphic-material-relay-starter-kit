{
  "name": "isomorphic-material-relay-starter-kit",
  "description": "Starter kit for isomorphic SPA using React/Relay, Material-UI, JWT authentication and samples",
  "author": "Aleksandar Chalakov and Code Foundries",
  "license": "MIT",
  "homepage": "https://github.com/codefoundries/isomorphic-material-relay-starter-kit",
  "version": "0.6.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codefoundries/isomorphic-material-relay-starter-kit.git"
  },
  "keywords": [
    "SPA",
    "single page application",
    "isomorphic",
    "progressive enhancement",
    "server rendering",
    "authentication",
    "JWT",
    "Material Design",
    "material-ui",
    "react",
    "relay",
    "isomorphic-relay",
    "isomorphic-relay-router",
    "graphql",
    "react-hot-loader",
    "express",
    "webpack",
    "babel",
    "babel 6"
  ],
  "engines": {
    "node": "4.2.4",
    "npm": "3.3.6"
  },
  "scripts": {
    "postinstall": "npm run build-schema && npm run build-webpack",
    "setup-local": "cp ./doc/Example.env ./.env",
    "build-schema": "./node_modules/.bin/babel-node ./scripts/build-schema.js",
    "build-webpack": "./node_modules/.bin/webpack -p --output-filename app.js",
    "build-mui-icon-list": "./node_modules/.bin/babel-node ./scripts/build-mui-icon-list.js",
    "start-webpack": "./node_modules/.bin/webpack-dev-server --hot --inline --progress --colors",
    "start-dev": "export NODE_ENV=development && nodemon --exec ./node_modules/.bin/babel-node -- ./server/server.js",
    "start": "export NODE_ENV=production && ./node_modules/.bin/babel-node ./server/server.js"
  },
  "metadata": {
    "graphql": {
      "schema": "./graphql/schema.json"
    }
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-transform-async-to-generator": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-relay-plugin": "^0.6.3",
    "babel-relay-plugin-loader": "^0.6.3",
    "babel-runtime": "^6.3.19",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.1",
    "cassandra-driver": "^3.0.0",
    "classnames": "^2.2.0",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.19.0",
    "dotenv": "^1.2.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-graphql": "^0.4.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "graphql": "^0.4.12",
    "graphql-relay": "^0.3.4",
    "isomorphic-relay": "^0.4.2",
    "isomorphic-relay-router": "^0.5.1",
    "jwt-simple": "^0.4.0",
    "material-ui": "^0.14.0",
    "nib": "^1.1.0",
    "node-fetch": "^1.3.3",
    "nodemon": "^1.7.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-helmet": "^2.2.0",
    "react-hot-loader": "^1.3.0",
    "react-relay": "^0.6.1",
    "react-router": "^2.0.0-rc4",
    "react-tap-event-plugin": "^0.2.1",
    "seq-queue": "0.0.5",
    "squel": "^4.2.2",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.3.13",
    "recursive-readdir-sync": "^1.0.6",
    "webpack-dev-server": "^1.14.0"
  }
}
